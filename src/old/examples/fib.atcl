FUNCTION PRINT_NL
    MOVE 3
    SET 10
    PRINT
FUNCTION COUNT
    MOVE 0
    VAL
    CALL PRINT_NL
    MOVE 1
    ADD 0
    MOVE 0
    ADD 1
    MOVE 1
    VAL
    MOVE 2
    DEC 1
    CALL PRINT_NL

FUNCTION MAIN
    MOVE 0
    MOVE 1 SET 1 
    MOVE 0 SET 0
    MOVE 2
    SET 20
    WHILE 2 != 0 COUNT
