--NEW--
FUNCTION PASS
	PASS
--NEW--
FUNCTION PRINTNEWLINE
	MOVE 6
	SET 10
	PRINT
	MOVE 0
--NEW--
FUNCTION PRINTZERO 
	MOVE 2 
	SET 0
	VAL
	CALL PRINTNEWLINE
--NEW--
FUNCTION PRINTONE
	MOVE 3
	SET 1
	VAL
	MOVE 0
	CALL PRINTNEWLINE
--NEW--
FUNCTION LOOP
	MOVE 5 
	SET 0
	WHILE 5 IS 0 PRINTONE
--NEW--
FUNCTION MAIN
	MOVE 0
	RDINT
	IF 0 IS 1 LOOP PASS
	IF 0 IS 0 PRINTZERO PASS
